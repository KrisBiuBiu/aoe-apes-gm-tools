<h3 class="form-signin-heading"><%= ctx %></h3>
<form action="<%= url %>" class="form-signin" method="post">
<input type="text" class="input-block-level" id="region" name="region" value="1" req="true" placeholder="大区ID" exp="^[1-9]\d*$" ts='大区ID格式错误'>
<input type="text" class="input-block-level" id="userids" name="userids" req="true" placeholder="玩家ID列表 , 分隔" exp="^([1-9]\d*\,){0,}([1-9]\d*)$" ts='玩家ID列表格式错误'>
<input type="text" class="input-block-level" id="titlekey" name="titlekey" req="true" placeholder="邮件标题ID" exp="^[1-9]\d*$" ts='邮件标题ID格式错误'>
<input type="text" class="input-block-level" id="contentkey" name="contentkey" req="true" placeholder="邮件内容ID" exp="^[1-9]\d*$" ts='邮件内容ID格式错误'>
<button class="btn btn-large btn-primary" type="submit">提交</button>
  <% if (csrf) { %>
<input type="hidden" name="_csrf" value='<%= csrf %>'>
  <% } %>
</form>

<script type="text/javascript">
    F.st();
</script>